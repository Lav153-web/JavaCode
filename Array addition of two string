import java.io.*;
import java.util.*;
import java.lang.*;
class StringArray
{
public static void main(String args[])
{
String nm1[]=new String[]{"a","b","c"};
String nm2[]=new String[]{"d","e","f"};
for(int i=0;i<nm1.length;i++)
{
System.out.print(nm1[i]);
}
for(int i=0;i<nm2.length;i++)
{
System.out.print(nm2[i]);
}
}
}

